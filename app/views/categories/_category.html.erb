<div class="text-center">
    <% obj.each do |c| %>
        <ul class="listing">
            <div class="row">
                <div class="col-md-4 col-md-offset-4">
                    <div class="well well-lg text-black">
                        <li class="category-title"><%= link_to c.name, category_path(c) %></li>
                    </div>
                </div>
            </div>
        </ul>
    <% end %>
</div>